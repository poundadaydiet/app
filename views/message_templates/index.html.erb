<h1>Listing message_templates</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Subject</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @message_templates.each do |message_template| %>
  <tr>
    <td><%= message_template.name %></td>
    <td><%= message_template.description %></td>
    <td><%= message_template.subject %></td>
    <td><%= message_template.content %></td>
    <td><%= link_to 'Show', message_template %></td>
    <td><%= link_to 'Edit', edit_message_template_path(message_template) %></td>
    <td><%= link_to 'Destroy', message_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Message template', new_message_template_path %>
